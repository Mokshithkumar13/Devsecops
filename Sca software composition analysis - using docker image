stages:
   - sca
sca:
    stage: sca
    image:
        name: aquasec/trivy
        entrypoint: [""]
    script:
        - trivy repository . --scanners vuln -f json -o sca-scan-report.json 
    artifacts:
        when: always
        paths:
            - "sca-scan-report.json"


